			function(e) {
				e.preventDefault();
				CalendarController.setMonth(-1);
			}
		);
		NTES("#nextMonth").addEvent(
			"click",
			function(e) {
				e.preventDefault();
				CalendarController.setMonth(1);
			}
		);
	},
	
	setMonth : function(relative) {
		var year = this._now.getFullYear(), month = this._now.getMonth() + 1 + relative;
		if (month > 12) {
			year++;
			month = month - 12;
		} else if (month <= 0) {
			year--;
			month = month + 12;
		}
		this._now.setFullYear(year, month - 1);
		NTES("#setYear").innerHTML = year;
		NTES("#setMonth").innerHTML = month;
		this._calendar.setDate(year, month);
		NTES("#calendarList").innerHTML = NTES.ui.Template.parse("calendar", this._calendar.build());
	}
};
CalendarController.init();
new NTES.ui.PopupLayer(NTES("#getCalendar"), NTES("#calendarBox"), "click");
